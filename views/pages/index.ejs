  <section class="book_section layout_padding" id="book">

    <div class="mx-auto container">
      <div class="heading_container">
        <h2>
          Book A Table
        </h2>
      </div>
      <div class="row">
        <div class="col-md-6">
          <div class="form_container">
            <form id="checkout-form" method="get" action="/book">
              <div >
                <input type="text" class="form-control name" id="checkout-name" name="name" required
                  placeholder="Name" />
              </div>

              <div >
                <input type="tel" class="form-control mobile" id="checkout-phone" name="phone" required
                  placeholder="Mobile Number" />
              </div>

              <div >
                <input type="email" class="form-control emailid" id="checkout-email" name="email" required
                  placeholder="Email Id" />

              </div>

              <div>
                <select class="form-control nice-select wide personcount" compulsary>
                  <option value="" disabled selected>
                    Person Count
                  </option>
                  <option value="">
                    1
                  </option>
                  <option value="">
                    2
                  </option>
                  <option value="">
                    3
                  </option>
                  <option value="">
                    4
                  </option>
                  <option value="">
                    5
                  </option>
                </select>
              </div>



              <div >
                <input type="date" class="form-control" name="date" placeholder="Date" required>

              </div>

              <div >
                <input type="submit" class="btn" id="checkout-btn" name="Book Now" value="Book Now">
              </div>

            </form>
          </div>
        </div>
        <div class="col-md-6">
          <div class="map_container ">
            <div id="googleMap" class="googleMap">
              <p><iframe
                  src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=en&amp;geocode=&amp;q=iiit+hyderabad&amp;sll=35.227672,-103.513184&amp;sspn=10.331413,22.258301&amp;ie=UTF8&amp;ll=17.456128,78.352175&amp;spn=0.011791,0.021737&amp;z=14&amp;output=embed"
                  width="675" height="400" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"></iframe></p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

